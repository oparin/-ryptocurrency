{% if wo_breadcrumbs()|length %}
    {%- spaceless -%}
        <ul id="{{ listId }}" class="{{ listClass }}" itemscope itemtype="http://data-vocabulary.org/Breadcrumb">
            {% for b in breadcrumbs %}
                <li{% if itemClass is defined and itemClass|length and not loop.last %} class="{{ itemClass }}" {% else %} class="current"{% endif %}{% if not(loop.first) %} itemprop="child"{% endif %}>
                    {% if b.url and not loop.last %}
                        <a href="{{ b.url }}">{{- b.text | trans(b.translationParameters, translation_domain, locale) -}}</a>
                    {% else %}
                        <a href="#">{{- b.text | trans(b.translationParameters, translation_domain, locale) -}}</a>
                    {% endif %}
                </li>
            {% endfor %}
        </ul>
    {%- endspaceless -%}
{% endif %}
